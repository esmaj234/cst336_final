<!-- search.ejs -->
<%- include('partials/header.ejs') %>
  <h1>Search Results</h1>

  <% if (results.length > 0) { %>
    <ul>
      <% results.forEach(product => { %>
        <li><%= product.productname %> - <%= product.productprice %></li>
      <% }); %>
    </ul>
  <% } else { %>
    <p>No products found.</p>
  <% } %>

<%- include('partials/footer') %>
